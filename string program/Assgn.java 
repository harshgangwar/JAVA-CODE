import java.util.Scanner;
public Assgn
{
	static int flag=0;
	static int wordCount(String h)
	{
		for(int i=0;i<h.length();i++)
		{
			if(h.charAt(i)=='')
			{
				flag++;
			}

		}
		return flag;
	}


	public static void main(String... harsh)
	{
		Scanner sc=new Scanner(System.in);
		String in=sc.nextLine();
		int re=wordCount(in);
		System.out.println("result is  +" re);
	}
}